{
  "name": "mood-up-api",
  "version": "1.0.0",
  "description": "Mood Up Rest Api",
  "main": "app.js",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "start:debug": "node --nolazy --inspect=38009 -r ts-node/register ./src/server",
    "start:test": "ts-mocha ./src/tests/*.spec.ts --exit --timeout 60000",
    "start:watch": "nodemon --config './nodemon.json'",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "build": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "author": "wojdanowski",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.810.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-mongo-sanitize": "2.0.1",
    "express-rate-limit": "5.2.3",
    "express-validator": "6.9.2",
    "helmet": "4.2.0",
    "http-status-codes": "2.1.4",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.11.6",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "passport": "0.4.1",
    "passport-headerapikey": "1.2.2",
    "passport-http-bearer": "1.0.1",
    "util": "0.12.3",
    "uuid": "8.3.2",
    "validator": "13.5.2",
    "xss-clean": "0.1.1"
  },
  "engines": {
    "node": "14.15.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "4.2.14",
    "@types/chai-http": "4.2.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.9",
    "@types/expect": "24.3.0",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "5.1.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "8.2.0",
    "@types/mongoose": "5.10.3",
    "@types/morgan": "1.9.2",
    "@types/multer": "1.4.5",
    "@types/multer-s3": "2.7.9",
    "@types/node": "14.14.22",
    "@types/passport": "1.0.5",
    "@types/passport-http-bearer": "1.0.36",
    "@types/redis": "2.8.28",
    "@types/sinon": "9.0.10",
    "@types/uuid": "8.3.0",
    "@types/validator": "13.1.3",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "decache": "4.6.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mocha": "8.2.1",
    "mongodb-memory-server": "6.9.3",
    "prettier": "2.2.1",
    "sinon": "9.2.3",
    "ts-mocha": "8.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}